{
  "name": "ogc",
  "private": true,
  "version": "0.0.0",
  "description": "API Client for OGC Web Services",
  "main": "src/index.js",
  "scripts": {
    "build": " && rimraf ./dist/*.ts ./dist/*/*.ts",
    "clean": "rimraf ./dist",
    "build:cpy": "cpy '.' '../dist/' --cwd=src",
    "build:tsc": "tsc ./dist/*.ts ./dist/*/*.ts ./dist/*/*/*.ts",
    "f": "npm run format",
    "format": "npx prettier --arrow-parens=avoid --print-width=80 --trailing-comma=none --write ./src/*/*.ts ./src/*/*/*.ts",
    "test:base": "npx ts-node --esm --skipProject ./src/base/test.ts",
    "test:common": "npx ts-node --esm --skipProject ./src/common/test.ts",
    "test:gml": "npx ts-node --esm --skipProject ./src/gml/test.ts",
    "test:ows": "npx ts-node --esm --skipProject ./src/ows/test.ts",
    "test:wcs": "npx ts-node --esm --skipProject ./src/wcs/utils/test.ts && npx ts-node --esm --skipProject ./src/wcs/WCS/test.ts",
    "test:wfs": "npx ts-node --esm --skipProject ./src/wfs/test.ts",
    "test:wms": "npx ts-node --esm --skipProject ./src/wms/utils/test.ts && npx ts-node --esm --skipProject ./src/wms/WMS/test.ts",
    "test": "npm run test:base && npm run test:common && npm run test:gml && npm run test:ows && npm run test:wcs && npm run test:wfs && npm run test:wms"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/ogc.git"
  },
  "keywords": [
    "test",
    "ogc"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/ogc/issues"
  },
  "homepage": "https://github.com/DanielJDufour/ogc#readme",
  "devDependencies": {
    "cpy-cli": "^4.1.0",
    "cross-fetch": "^3.1.5",
    "find-and-read": "^1.2.0",
    "flug": "^2.3.1",
    "rimraf": "^3.0.2",
    "srvd": "^0.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "xml-utils": "^1.3.0-0"
  }
}
